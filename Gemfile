# A sample Gemfile
source :rubygems 
gemspec
gem "active_column", :path => "local_gems/active_column"
gem "cassandra", :git => "https://github.com/fauna/cassandra.git"

group :development do
  gem "shotgun"
  gem "awesome_print" 
end

group :test do
  gem "rake"
  gem "rack-test"
  gem "minitest"
end
